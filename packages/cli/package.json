{
  "name": "@ciagent/cli",
  "version": "0.1.0",
  "description": "CLI interface for ciagent - vendor-neutral AI agent tool",
  "type": "module",
  "main": "./src/cli.ts",
  "bin": {
    "cia": "./src/cli.ts"
  },
  "scripts": {
    "type-check": "bun tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "build": "bun build --compile --minify --sourcemap --bytecode src/cli.ts --outfile ../../dist/cia"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.7",
    "@openai/codex-sdk": "^0.87.0",
    "ajv": "^8.17.1",
    "p-retry": "7.1.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/json-schema": "^7.0.15",
    "vitest": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0"
  },
  "keywords": [
    "cli",
    "ai",
    "agent",
    "cicd"
  ],
  "author": "ciagent",
  "license": "MIT"
}
