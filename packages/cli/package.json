{
  "name": "@ciagent/cli",
  "version": "0.1.0",
  "description": "CLI interface for ciagent - vendor-neutral AI agent tool",
  "type": "module",
  "main": "./src/cli.ts",
  "bin": {
    "cia": "./src/cli.ts"
  },
  "scripts": {
    "type-check": "bun tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "build": "bun run build:release",
    "build:dev": "bun build --compile --minify --sourcemap --bytecode src/cli.ts --outfile ../../dist/cia",
    "build:release": "bun build --compile --minify --bytecode src/cli.ts --outfile ../../dist/cia",
    "lint": "eslint src/"
  },
  "dependencies": {
    "@ai-sdk/azure": "^3.0.30",
    "@ai-sdk/openai": "^3.0.30",
    "@anthropic-ai/claude-agent-sdk": "^0.2.7",
    "@anthropic-ai/sdk": "^0.74.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@openai/codex-sdk": "^0.104.0",
    "@types/node": "^25.2.3",
    "ai": "^6.0.86",
    "ajv": "8.18.0",
    "open": "^11.0.0",
    "p-retry": "7.1.1",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/json-schema": "^7.0.15",
    "@vitest/coverage-v8": "^1.6.0",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "cli",
    "ai",
    "agent",
    "cicd"
  ],
  "author": "ciagent",
  "license": "MIT"
}
